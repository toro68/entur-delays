# Port for the local dashboard
PORT=5173

# Entur JourneyPlanner v3 GraphQL
ENTUR_ENDPOINT=https://api.entur.io/journey-planner/v3/graphql

# Required header for open access
ENTUR_CLIENT_NAME=sa-entur-internal-delay-dashboard

# Optional: polling interval (ms)
REFRESH_MS=30000

# Optional: limit number of rows shown
TOP_N=25

# Optional filters
TRANSPORT_MODE=bus

